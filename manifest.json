{
  "manifest_version": 2,
  "name": "GTmetrix Analyzer Plugin",
  "description": "This extension will analyze a page using GTmetrix",
  "version": "2.0",

  "content_scripts": [{
    "matches": ["https://open.spotify.com/*"],
    "js":      ["content.js"]
  }],

  "browser_action": {
    "default_popup": "popup.html"
  }

  "permissions": ["activeTab"]
}